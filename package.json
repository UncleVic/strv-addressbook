    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "ajv": "^8.0.5",
    "ajv-formats": "^2.0.2",
    "bcrypt": "^5.0.1",
    "eslint": "^7.23.0",
