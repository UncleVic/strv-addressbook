    "@types/mongodb": "^3.6.12",
    "@types/node": "^14.14.37",
    "@types/serve-static": "^1.13.9",
    "@types/uuid": "^8.3.0",
    "dotenv": "^8.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
